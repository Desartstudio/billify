autoload:

  -
    name:        symfony core classes
    ext:         .class.php
    path:        %SF_SYMFONY_LIB_DIR%/symfony
    recursive:   on
    exclude:     [module]

  -
    name:         symfony core classes (continued)
    files:
      Propel:     %SF_SYMFONY_LIB_DIR%/addon/propel/sfPropelAutoload.php
      Criteria:   %SF_SYMFONY_LIB_DIR%/propel/util/Criteria.php
      Creole:     %SF_SYMFONY_LIB_DIR%/vendor/creole/Creole.php

  -
    name:         project classes
    ext:          .php
    path:         %SF_LIB_DIR%
    recursive:    on
    exclude:      [model]

  -
    name:         project model classes
    ext:          .php
    path:         %SF_LIB_DIR%/model

  -
    name:         application classes
    ext:          .class.php
    path:         %SF_APP_DIR%/lib
    
  -
    name:		  DomPdf
    ext:          .cls.php
    path:         %SF_APP_DIR%/lib/dompdf/include

  -
    name:        pake classes
    ext:         .class.php
    path:        %SF_SYMFONY_LIB_DIR%/../pake
    
  -    
    name:		  mMenu
    ext:          .php
    path:         %SF_APP_DIR%/lib/menu/extras/php